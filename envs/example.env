# Snowflake Connection Parameters
# Required Parameters
SNOWFLAKE_ACCOUNT=your_account_identifier       # Format: orgname-accountname (e.g., xy12345.us-east-1)
SNOWFLAKE_USER=your_username                    # Your Snowflake username
SNOWFLAKE_PASSWORD=your_password                # Your Snowflake password

# Optional Parameters (comment out if not needed)
SNOWFLAKE_ROLE=ACCOUNTADMIN                     # Default role to use for connection
SNOWFLAKE_WAREHOUSE=COMPUTE_WH                  # Default warehouse to use
SNOWFLAKE_DATABASE=YOUR_DB                      # Default database to connect to
SNOWFLAKE_SCHEMA=PUBLIC                         # Default schema to use

# Connection Settings
SNOWFLAKE_REGION=us-east-1                      # Your Snowflake account region
SNOWFLAKE_CLIENT_SESSION_KEEP_ALIVE=True        # Keep connection alive during idle periods

# Performance Settings
SNOWFLAKE_QUERY_TAG=myapp                       # Tag to identify queries from this application
SNOWFLAKE_CLIENT_PREFETCH_THREADS=4             # Number of threads for prefetching results
SNOWFLAKE_CLIENT_MEMORY_LIMIT=1024              # Memory limit in MB for client side operations

# Security Settings (optional)
SNOWFLAKE_PRIVATE_KEY_PATH=/path/to/key.p8      # Path to private key if using key pair authentication
SNOWFLAKE_PRIVATE_KEY_PASSPHRASE=passphrase     # Passphrase for private key if encrypted

# Application Specific Settings
APP_LOG_LEVEL=INFO                              # Logging level for the application
APP_MAX_RETRIES=3                               # Maximum number of connection retry attempts
APP_TIMEOUT=300                                 # Connection timeout in seconds